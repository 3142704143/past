<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mox.mapper.user.AoaUserLoginRecordMapper">
  <resultMap id="BaseResultMap" type="com.mox.entity.user.AoaUserLoginRecord">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="record_id" jdbcType="BIGINT" property="recordId" />
    <result column="browser" jdbcType="VARCHAR" property="browser" />
    <result column="ip_addr" jdbcType="VARCHAR" property="ipAddr" />
    <result column="login_time" jdbcType="TIMESTAMP" property="loginTime" />
    <result column="session_id" jdbcType="VARCHAR" property="sessionId" />
    <result column="user_id" jdbcType="BIGINT" property="userId" />
  </resultMap>
  
  <resultMap type="com.mox.entity.user.AoaUserLoginRecord" id="myResultMap" extends="BaseResultMap">
  	<result column="user_name" jdbcType="BIGINT" property="userName" />
  </resultMap>
  <select id="queryAll" resultMap="myResultMap">
  	SELECT b.*,a.user_name FROM `aoa_user` a  INNER JOIN aoa_user_login_record b WHERE a.user_id=b.user_id
  </select>
  
  <select id="queryBySearch"  parameterType="String" resultMap="myResultMap">
  	SELECT b.*,a.user_name FROM `aoa_user` a  INNER JOIN aoa_user_login_record b
   WHERE a.user_id=b.user_id 
   and a.user_name like concat('%',#{name},'%') 
  </select>
  
</mapper>